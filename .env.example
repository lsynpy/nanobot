# nanobot deployment configuration
# Copy this file to .env and edit with your values

# Aliyun Container Registry namespace (your namespace, not login email)
ALIYUN_NAMESPACE="your-namespace"

# Aliyun region
REGION="cn-hangzhou"

# Target VPS/Router hostname (configured in ~/.ssh/config)
VPS_HOSTNAME="your-vps"

# Gateway port
GATEWAY_PORT="18790"

# Platform architecture (multi-arch build)
PLATFORM="linux/arm64,linux/amd64"
